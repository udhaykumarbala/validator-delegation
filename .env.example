# Copy this file to .env and fill in your values

# Private key for deploying contracts (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# RPC URLs
TESTNET_RPC_URL=https://evmrpc-testnet.0g.ai
MAINNET_RPC_URL=https://evmrpc.0g.ai

# Chain IDs
MAINNET_CHAIN_ID=20000

# Explorer API Key (if available)
EXPLORER_API_KEY=placeholder

# Foundation wallet address (for admin operations)
FOUNDATION_WALLET_ADDRESS=0x...

# Contract addresses after deployment (will be filled after deployment)
MOCK_STAKING_CONTRACT_ADDRESS=
MOCK_STAKING_CONTRACT_NETWORK=